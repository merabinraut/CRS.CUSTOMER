@*club section end Host image section*@
<div id="tab2" class="tab-content-plane exclude-class" style="display:none;">
    @foreach (var item in Model.HostListModel)
    {
        <div class="flex flex-row gap-[16px] mx-[16px]">
            <div style="border-radius: 4px;" class="w-[128px] mt-[16px] cursor-pointer h-auto flex  justify-end text-center relative">
                <div class="w-full flex justify-center" onclick="HostDetailRedirectFunction('@item.HostCode', '@item.ClubCode', '@item.LocationURL');">
                    <img class="w-[128px] h-[180px] object-cover rounded-[2px]"
                         src="@item.HostImage" />
                </div>
