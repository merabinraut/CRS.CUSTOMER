@model CRS.CUSTOMER.APPLICATION.Models.LocationManagement.LocationClubHostModel

<style>

    /* decorate */
    .disabled {
        color: #aaaaaa !important;
        background: #d7d7d7 !important;
    }

    .gradient {
        display: flex;
        justify-content: center;
        align-items: center;
        background: linear-gradient(96deg, #99aefb 10%, #f04aa4 61.32%, #e7b282 98.78%);
    }

    .list_wrap {
        width: 100%;
        height: auto;
        display: block;
        float: left;
        background: #efefef;
    }

    .club_content {
        width: 100%;
        height: auto;
        margin: 10px 0 0 0;
        padding: 15px 0 0 0;
        display: block;
        float: left;
        background: #fff;
    }

        .club_content:first-child {
            margin-top: 0;
        }

        .club_content:last-child {
            margin-bottom: 0;
        }

        .club_content a {
            width: 100%;
            display: block;
        }

    ul.club_tags, .club_header, .club_rate, .club_pr {
        padding-left: 15px;
        padding-right: 10px;
    }
    /* tags */
    ul.club_tags {
        width: 100%;
        height: auto;
        margin: 0 0 10px 0;
        display: inline-flex;
        flex-wrap: wrap;
    }

        ul.club_tags li {
            width: auto;
            height: auto;
            margin: 5px 0 0 5px;
            padding: 2px 8px;
            display: inline-flex;
            align-items: center;
            color: #ffffff;
            font-size: 10px;
            font-weight: 600;
            border-radius: 3px;
        }

            ul.club_tags li:first-child {
                margin-left: 0;
            }

            ul.club_tags li.tag_location {
                padding: 0;
                color: #aaaaaa;
                font-size: 0.8rem;
            }

            ul.club_tags li.tag_rank {
                background: #BFA3B6;
            }

            ul.club_tags li.tag_gold {
                background: #d4b78a;
            }

            ul.club_tags li.tag_good {
                background: #96b41f;
            }

            ul.club_tags li.tag_platinum {
                background: #697DBF;
            }

            ul.club_tags li.tag_big {
                background: #efb726;
            }
    /* hostclub info */
    .club_header {
        width: 100%;
        height: auto;
        display: flex;
        justify-content: space-between;
    }

    .club_logo {
        width: 45px;
        height: 45px;
        margin: 0 10px 0 0;
        padding: 0;
        display: inline-flex;
    }

        .club_logo img {
            width: 100%;
            height: 100%;
            border: 1px solid #e5e7eb;
            border-radius: 50%;
            background: #efefef;
            object-fit: cover;
        }

    .club_info {
        width: calc(100% - 100px);
        height: auto;
        max-height: 45px; /* as 「.club_logo」height */
        display: flex;
        flex-wrap: wrap;
        align-items: center;
    }

    h3.club_name, p.club_kana, p.club_group {
        display: inline-flex;
        align-items: center;
    }

    .club_info h3, .club_info p {
        width: 100%;
        font-size: 0.7rem;
        font-weight: 600;
    }

    h3.club_name {
        height: 19px;
        font-size: 0.9rem;
    }

    p.club_kana, p.club_group {
        height: 13px;
        color: #aaaaaa;
    }

    p.club_group {
        color: #e3a1a1;
        font-weight: normal;
    }

    .action_keep {
        width: 33px;
        height: 33px;
        padding: 1px;
        display: inline-flex;
        flex-flow: wrap-reverse;
    }

        .action_keep img {
            width: 33px;
            height: 33PX;
            aspect-ratio: 1 / 1;
        }
    /* hostclub thumb */
    ul.club_thumb {
        width: 100%;
        height: auto;
        margin: 10px 0;
        display: flex;
        flex-wrap: nowrap;
    }

        ul.club_thumb li {
            width: calc(100% / 3);
            height: auto;
            margin: 0;
            padding: 0 0 0 2px;
            display: inline-flex;
            aspect-ratio: 1 / 1;
        }

            ul.club_thumb li:first-child {
                padding: 0;
            }

            ul.club_thumb li img {
                width: 100%;
                height: 100%;
                aspect-ratio: 1 / 1;
                object-fit: cover;
                background: #efefef;
            }
    /* rate */
    .club_rate, ul.rate_stars, ul.rate_stars li, ul.rate_review {
        height: 18px; /* as 「ul.rate_stars li」 width */
    }

    .club_rate {
        width: 100%;
        height: 18px;
        display: flex;
        flex-wrap: nowrap;
        font-size: 1.1rem;
    }

    ul.rate_stars {
        display: flex;
        justify-content: flex-start;
        align-items: center;
    }

        ul.rate_stars li {
            width: 18px;
            margin: 0 0 0 2px;
            padding: 0;
            display: inline-flex;
            align-items: center;
        }

            ul.rate_stars li:first-child {
                margin: 0;
            }

            ul.rate_stars li img {
                width: 100%;
                height: 100%;
                object-fit: cover;
                aspect-ratio: 1 / 1;
            }

            ul.rate_stars li.rate_value {
                width: auto;
                height: 18px;
                margin: 0 0 0 8px;
                padding: 0;
                font-size: 20px;
                font-weight: bold;
                letter-spacing: 0;
            }

    ul.rate_review {
        width: auto;
        margin: 0 0 0 10px;
        display: inline-flex;
    }

        ul.rate_review li.comment_value {
            width: auto;
            height: 20px;
            margin: 0 0 0 2px;
            padding: 0;
            display: inline-flex;
            align-items: center;
            font-size: 14px;
        }

        ul.rate_review li:first-child {
            width: 20px;
            height: 20px;
        }

    .rate_review img {
        width: 100%;
        height: 100%;
        aspect-ratio: 1 / 1;
        object-fit: cover;
    }

    p.club_pr {
        width: 100%;
        height: 50px;
        display: inline-flex;
        flex-wrap: wrap;
        align-items: center;
        font-size: 0.75rem;
        overflow: hidden;
        line-height: 1.4;
        text-overflow: ellipsis;
    }
    /* hostclub footer */
    .club_footer {
        width: 100%;
        height: auto;
        margin: 10px 0 0 0;
        padding: 15px 15px 20px 15px;
        display: flex;
        justify-content: space-between;
        border-top: 1px solid #e5e7eb;
    }

    .club_status {
        width: 120px;
        height: auto;
        margin: 0;
        display: inline-flex;
        flex-flow: column;
    }

    .opening_hours {
        width: 140px;
        height: 18px;
        display: inline-flex;
        align-items: center;
        font-size: 0.85rem;
        font-weight: 600;
        letter-spacing: 0.02rem;
    }

        .opening_hours img {
            width: 18px;
            height: 18px;
            margin: 0 5px 0 0;
            display: inline-flex;
            align-items: center;
        }

    .available {
        width: 100%;
        height: auto;
        margin: 5px 0 0 0;
        padding: 1.4px;
        display: inline-flex;
        justify-content: center;
        align-items: center;
        color: #ffffff;
        font-size: 0.8rem;
        font-weight: 600;
        border-radius: 3px;
        background: #d4b78a;
    }

    .club_footer .rsv_btn {
        max-width: 173px;
        width: calc(100% - 135px);
        height: 45px;
        color: #ffffff;
        font-size: 1rem;
        font-weight: 600;
        display: inline-flex;
        letter-spacing: 1px;
        border-radius: 4px;
    }

    .book-now-btn {
        display: flex;
        justify-content: center;
        gap: 1px;
        width: 100%;
        flex-grow: 1;
    }

    .call-now-button {
        border-radius: 4px;
        border: 1px solid #D75A8B;
        display: flex !important;
        justify-content: center;
        gap: 4px;
        width: 100%;
        max-width: 84px;
        align-items: center;
        color: #D75A8B;
        font-size: 14px;
        font-weight: 600;
        letter-spacing: 0.56px;
        line-height: 150%;
        height: 45px;
    }

    .blocked-btn {
        border-radius: 4px;
        border: 1px solid #F1F1F1;
        background: #F0F0F0;
        max-width:173px;
    }
    .rsv_btn{
        max-width:173px;
    }

    .list_wrap {
        background: #f8f8f8;
    }

    @@media(width > 639px) {
        .responsiveReviewModalPopup {
            max-width: 375px;
            width: 100%;
            margin: 0 auto;
        }
    }
</style>

<div id="tab1" class="tab-content-plane  exclude-class">
    <div class="list_wrap">
        @foreach (var item in Model.ClubListModel)
        {
            if (item.ClubCategory.Trim().ToUpper() == "P")
            {
                /* new improvememnt*/
                <div class="club_content cursor-pointer">
                    <!-- s/link -->
                    <a onclick="ClubDetailRedirectFunction('@item.ClubCode', '@item.LocationURL')">
                        <!-- s/club_tag -->
                        <ul class="club_tags">
                            @if (!string.IsNullOrEmpty(@item.Tag1))
                            {
                                <!-- s/tag -->
                                <li class="tag_location"><img class="map_pin" src="~/Content/assets/images/location_icon.svg" alt="ピン"> @item.Tag1</li>
                                <!-- e/tag -->
                            }
                            @if (!string.IsNullOrEmpty(@item.Tag2))
                            {
                                <!-- s/tag -->
                                <li class="tag_rank">
                                    ランク @item.Tag2
                                </li>
                            }
                            <!-- e/tag -->
                            <!-- s/tag -->
                            @if (!string.IsNullOrEmpty(@item.Tag3))
                            {
                                <li class="tag_platinum">  @item.Tag3</li>
                            }

                            @if (!string.IsNullOrEmpty(@item.Tag4))
                            {
                                <!-- e/tag -->
                                <!-- s/tag -->
                                <li class="tag_good">@item.Tag4</li>
                            }
                            @if (!string.IsNullOrEmpty(@item.Tag5))
                            {
                                <li class="tag_big">  @item.Tag5</li>
                            }
                            <!-- e/tag -->
                        </ul>
                        <!-- e/club_tag -->
                        <!-- s/club header -->
                        <div class="club_header">
                            <!-- s/club logo -->
                            <div class="club_logo"><img class="logo" alt="ロゴ" src="@item.ClubLogo"></div>
                            <!-- e/club logo -->
                            <!-- s/club info -->
                            <div class="club_info">
                                <!-- s/club_name -->
                                <h3 class="club_name">@item.ClubName</h3>
                                <!-- e/club_name -->
                                <!-- s/club kana -->
                                <p class="club_kana">@item.ClubNameJapanese</p>
                                <!-- e/club kana -->
                                <!-- s/club group -->
                                @if (!string.IsNullOrEmpty(item.GroupName))
                                {
                                    <p class="club_group">@item.GroupName</p>
                                }
                                <!-- e/club group -->
                            </div>
                            <!-- e/club info -->
                            <!-- s/action btn -->
                            @if (item.IsBookmarked.ToUpper() == "Y")
                            {
                                <div class="action_keep" onclick="event.stopPropagation(); ManageBookmark('@item.ClubId', '', 'club', this)"><img alt="保存する" src="~/Content/assets/images/bookmark-filled.svg"></div>
                            }
                            else
                            {
                                <div class="action_keep" onclick="event.stopPropagation(); ManageBookmark('@item.ClubId', '', 'club', this)"><img alt="保存する" src="~/Content/assets/images/bookmark-notfilled.svg"></div>
                            }
                            <!-- e/action btn -->
                        </div>
                        <!-- e/club header -->
                        <!-- s/thumb area -->
                        <ul class="club_thumb">
                            <!-- s/club thumb -->
                            @foreach (var item2 in item.HostGalleryImage)
                            {
                                <li><img alt="@item.ClubName" src="@item2"></li>
                            }
                            <!-- e/club thumb -->
                            <!-- s/club thumb -->
                        </ul>
                        <!-- e/thumb area -->
                        <!-- s/club rate -->
                        <div class="club_rate">
                            <!-- s/club rank -->
                            <ul class="rate_stars">
                                <!-- s/rank star -->
                                @{
                                    var unratedScale = (5 - item.AverageRating);
                                    for (int i = 0; i < item.AverageRating; i++)
                                    {
                                        <li><img src="~/Content/assets/images/ratingstar-filled.svg" alt="未評価"></li>
                                    }
                                    for (int i = 0; i < unratedScale; i++)
                                    {
                                        <!-- e/rank star -->
                                        <!-- s/rank star -->
                                        <li><img src="~/Content/assets/images/ratingstar-notfilled.svg" alt="未評価"></li>
                                    }
                                }
                                <!-- s/rank star -->
                                <li class="rate_value"> @item.AverageRating</li>
                                <!-- e/rank star -->
                            </ul>
                            <!-- e/club rank -->
                            <!-- s/club review -->
                            <ul class="rate_review">
                                <!-- s/comment frame -->
                                <li><img src="~/Content/assets/images/comment.svg" alt="コメント"></li>
                                <!-- e/comment frame -->
                                <!-- s/comment value -->
                                <li class="comment_value">@item.TotalComment</li>
                                <!-- e/comment value -->
                            </ul>
                            <!-- e/club review -->
                        </div>
                        <!-- e/club rate -->
                        <!-- s/club pr -->
                        @if (!string.IsNullOrEmpty(item.ClubDescription))
                        {
                            <p class="club_pr"> @item.ClubDescription</p>
                        }
                        <!-- e/club pr -->
                    </a>
                    <!-- e/link -->
                    <!-- s/club footer -->
                    <div class="club_footer" style="align-items: center">
                        <!-- s/club status -->
                        <div class="club_status">
                            <!-- s/opening hours -->
                            @{
                                DateTime currentTime = DateTime.Parse(DateTime.Now.ToString("HH:mm"));
                            }
                            @if (!string.IsNullOrEmpty(@item.ClubOpeningTime) && !string.IsNullOrEmpty(@item.ClubClosingTime))
                            {
                                if (!string.IsNullOrEmpty(@item.ClubTimePeriod) && @item.ClubTimePeriod.Trim().ToUpper() == "DAY")
                                {
                                    <div class="opening_hours">
                                        <img src="~/Content/assets/images/customer/sun.svg" alt="夜">
                                        <span style="line-height:13px;">
                                            @item.ClubOpeningTime &#xff5e; @item.ClubClosingTime
                                        </span>
                                    </div>
                                }
                                else if (!string.IsNullOrEmpty(@item.ClubTimePeriod) && @item.ClubTimePeriod.Trim().ToUpper() == "NIGHT")
                                {
                                    <div class="opening_hours">
                                        <img src="/Content/assets/images/customer/moon.svg" alt="夜">
                                        <span style="line-height:13px;">
                                            @item.ClubOpeningTime &#xff5e; @item.ClubClosingTime
                                        </span>

                                    </div>
                                }
                                <!-- e/opening hours -->
                                <!-- s/available -->
                                if (!string.IsNullOrEmpty(item.TodaysClubSchedule) && item.TodaysClubSchedule.Trim().ToUpper() == "RESERVABLE")
                                {
                                    <div class="available">本日空席あり</div>
                                }
                                else
                                {
                                    <div class="available disabled">本日空席あり</div>
                                }
                            }
                            <!-- e/available -->
                        </div>
                        <!-- e/club status -->
                        <!-- call now button start -->
                        @*<a href="tel:@item.LandLine" class="call-now-button">
                                電話予約
                            </a>*@
                        <div onclick="callPopupModal('@item.ClubNameJapanese', '@item.ClubLogo', '@item.LandLine', '@item.Tag1', '@item.ClubCategory');"  class="call-now-button">
                            電話予約
                        </div>
                        <!-- call now button end -->
                        <!-- s/reservation btn -->
                        <button class="rsv_btn gradient book-now-btn" onclick="InitiateClubReservationFunction('@item.ClubId')">
                            ネット予約
                        </button>
                        <!-- e/reservation btn -->
                    </div>
                    <!-- e/club footer -->

                </div>
                /* new improvememnt end*/

            }
            else if (item.ClubCategory.Trim().ToUpper() == "B")
            {
                <div class="club_content cursor-pointer">
                    <a onclick="ClubDetailRedirectFunction('@item.ClubCode', '@item.LocationURL')">
                        <ul class="club_tags">
                            @if (!string.IsNullOrEmpty(@item.Tag1))
                            {
                                <li class="tag_location"><img class="map_pin" src="~/Content/assets/images/location_icon.svg" alt="ピン"> @item.Tag1</li>
                            }
                        </ul>
                        <!-- s/club header -->
                        <div class="club_header">
                            <!-- s/club logo -->
                            <div class="club_logo"><img class="logo" alt="ロゴ" src="@item.ClubLogo"></div>
                            <!-- e/club logo -->
                            <!-- s/club info -->
                            <div class="club_info">
                                <!-- s/club_name -->
                                <h3 class="club_name">@item.ClubName</h3>
                                <!-- e/club_name -->
                                <!-- s/club kana -->
                                <p class="club_kana">@item.ClubNameJapanese</p>
                                <!-- e/club kana -->
                                <!-- s/club group -->
                                <p class="club_group">@item.GroupName</p>
                                <!-- e/club group -->
                            </div>
                            <!-- e/club info -->
                            <!-- s/action btn -->
                            @if (item.IsBookmarked.ToUpper() == "Y")
                            {
                                <div class="action_keep" onclick="event.stopPropagation(); ManageBookmark('@item.ClubId', '', 'club', this)"><img alt="保存する" src="~/Content/assets/images/bookmark-filled.svg"></div>
                            }
                            else
                            {
                                <div class="action_keep" onclick="event.stopPropagation(); ManageBookmark('@item.ClubId', '', 'club', this)"><img alt="保存する" src="~/Content/assets/images/bookmark-notfilled.svg"></div>
                            }
                            <!-- e/action btn -->
                        </div>
                        <!-- e/club header -->
                        <!-- s/thumb area -->
                        <ul class="club_thumb">
                            <!-- s/club thumb -->
                            @foreach (var item2 in item.HostGalleryImage)
                            {
                                <li><img alt="@item.ClubName" src="@item2"></li>
                            }
                            <!-- e/club thumb -->
                            <!-- s/club thumb -->
                        </ul>
                        <!-- e/thumb area -->
                    </a>
                    <!-- e/link -->
                    <!-- s/club footer -->
                    <div class="club_footer">
                        <!-- s/club status -->
                        <div class="club_status">
                            <!-- s/opening hours -->
                            @if (!string.IsNullOrEmpty(@item.ClubOpeningTime) && !string.IsNullOrEmpty(@item.ClubClosingTime))
                            {
                                if (!string.IsNullOrEmpty(@item.ClubTimePeriod) && @item.ClubTimePeriod.Trim().ToUpper() == "DAY")
                                {
                                    <div class="opening_hours">
                                        <img src="~/Content/assets/images/customer/sun.svg" alt="夜">
                                        <span style="line-height:13px;">
                                            @item.ClubOpeningTime &#xff5e; @item.ClubClosingTime
                                        </span>
                                    </div>
                                }
                                else if (!string.IsNullOrEmpty(@item.ClubTimePeriod) && @item.ClubTimePeriod.Trim().ToUpper() == "NIGHT")
                                {
                                    <div class="opening_hours">
                                        <img src="/Content/assets/images/customer/moon.svg" alt="夜">
                                        <span style="line-height:13px;">
                                            @item.ClubOpeningTime &#xff5e; @item.ClubClosingTime
                                        </span>

                                    </div>
                                }
                                <!-- e/opening hours -->
                                <!-- s/available -->
                                if (!string.IsNullOrEmpty(item.TodaysClubSchedule) && item.TodaysClubSchedule.Trim().ToUpper() == "RESERVABLE")
                                {
                                    <div class="available">営業中</div>
                                }
                                else
                                {
                                    <div class="available disabled">営業中</div>
                                }
                            }
                            <!-- e/available -->
                        </div>
                        <!-- e/club status -->
                        <!-- call now button start -->
                        <a onclick="callPopupModal('@item.ClubNameJapanese', '@item.ClubLogo', '@item.LandLine', '@item.Tag1', '@item.ClubCategory');" class="call-now-button">
                           
                            電話予約
                        </a>
                        <!-- call now button end -->
                        <!-- s/reservation btn -->
                        <button class="rsv_btn gradient blocked-btn book-now-btn" onclick="InitiateClubReservationFunction('@item.ClubId')">
                            ネット予約
                        </button>
                        <!-- e/reservation btn -->
                    </div>
                    <!-- e/club footer -->
                </div>
            }
            <div style="background-color: #f8f8f8;" class=" h-[8px] my-[0px] py-[0px]">
            </div>
        }
    </div>
